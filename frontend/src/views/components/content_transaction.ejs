<div class="row">
    <h1>
        <%= data.title %>
    </h1>
    <hr>
    <div class="col">
        <table class="table table-striped table-hover">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <% data.fields.forEach((e,i)=> { %>
                        <th scope="col">
                            <%= e %>
                        </th>
                        <% }) %>
                            <!-- <th scope="col">#</th>
                    <th scope="col">Nit</th>
                    <th scope="col">Nombre</th>
                    <th scope="col">Fecha de Creacion</th>
                    <th scope="col">Fecha de Modificacion</th>
                    <th scope="col">Acciones</th> -->
                </tr>
            </thead>
            <tbody>
                <% data.data.forEach((e,i)=> { %>
                    <tr>
                        <th scope="row">
                            <%= i %>
                        </th>
                        <td>
                            <%= e.tipo %>
                        </td>
                        <td>
                            <%= e.idtransoriginal %>
                        </td>
                        <td>
                            <%= e.client.nombre %>
                        </td>
                        <td>
                            <%= e.createdAt %>
                        </td>
                        <td>
                            <%= e.updateAt %>
                        </td>
                    </tr>
                    <% }) %>
            </tbody>
        </table>
    </div>

</div>